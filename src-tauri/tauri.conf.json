{
  "productName": "dezmostenireapopeascatauri",
  "version": "0.1.0",
  "identifier": "com.dezmostenireapopeascatauri.app",
  "build": {
    "beforeDevCommand": "npm run start -- --host %HOST% --public-host %HOST%",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/dezmostenireapopeascatauri/browser"
  },
  "app": {
    "windows": [
      {
        "title": "dezmostenireapopeascatauri",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
